@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix orkgc: <http://www.orkg.org/orkg/ontology/class/> .
@prefix orkgp: <http://www.orkg.org/orkg/ontology/property/> .

#################################################################
# Classes
#################################################################
orkgc:Paper a owl:Class ;
    rdfs:label "Paper" .

orkgc:Contribution a owl:Class ;
    rdfs:label "Contribution" .

orkgc:Benchmark a owl:Class ;
    rdfs:label "Benchmark" .

orkgc:Dataset a owl:Class ;
    rdfs:label "Dataset" .

orkgc:Model a owl:Class ;
    rdfs:label "Model" .

orkgc:Evaluation a owl:Class ;
    rdfs:label "Evaluation" .

orkgc:Metric a owl:Class ;
    rdfs:label "Metric" .

orkgc:Author a owl:Class ;
    rdfs:label "Author" .

orkgc:Problem a owl:Class ;
    rdfs:label "Problem" .

orkgc:Venue a owl:Class ;
    rdfs:label "Venue" .

orkgc:ResearchField a owl:Class ;
    rdfs:label "ResearchField" .

# Measurement / Value classes
orkgc:C22007 a owl:Class ;
    rdfs:label "Measurement" .
orkgc:C22000 a owl:Class ;
    rdfs:label "R159178" .

#################################################################
# Object Properties
#################################################################

# Paper → Contribution
orkgp:P31 a owl:ObjectProperty ;
    rdfs:label "has Contribution" ;
    rdfs:domain orkgc:Paper ;
    rdfs:range orkgc:Contribution .

# Paper → ResearchField (rare / optional)
orkgp:P30 a owl:ObjectProperty ;
    rdfs:label "has ResearchField" ;
    rdfs:domain orkgc:Paper ;
    rdfs:range orkgc:ResearchField .

# Paper → Author
orkgp:P27 a owl:ObjectProperty ;
    rdfs:label "has Author" ;
    rdfs:domain orkgc:Paper ;
    rdfs:range orkgc:Author .

# Contribution → Benchmark
orkgp:HAS_BENCHMARK a owl:ObjectProperty ;
    rdfs:label "has Benchmark" ;
    rdfs:domain orkgc:Contribution ;
    rdfs:range orkgc:Benchmark .

# Contribution → Model (optional)
orkgp:HAS_MODEL a owl:ObjectProperty ;
    rdfs:label "has Model" ;
    rdfs:domain orkgc:Contribution ;
    rdfs:range orkgc:Model .

# Contribution → Problem
orkgp:P32 a owl:ObjectProperty ;
    rdfs:label "solves Problem" ;
    rdfs:domain orkgc:Contribution ;
    rdfs:range orkgc:Problem .

# Contribution → Venue (rare / optional)
orkgp:P29 a owl:ObjectProperty ;
    rdfs:label "published in Venue" ;
    rdfs:domain orkgc:Contribution ;
    rdfs:range orkgc:Venue .

# Contribution → Source Code (very rare)
orkgp:HAS_SOURCE_CODE a owl:ObjectProperty ;
    rdfs:label "has Source Code" ;
    rdfs:domain orkgc:Contribution ;
    # range unknown / optional

# Benchmark → Dataset
orkgp:HAS_DATASET a owl:ObjectProperty ;
    rdfs:label "has Dataset" ;
    rdfs:domain orkgc:Benchmark ;
    rdfs:range orkgc:Dataset .

# Benchmark → Evaluation
orkgp:HAS_EVALUATION a owl:ObjectProperty ;
    rdfs:label "has Evaluation" ;
    rdfs:domain orkgc:Benchmark ;
    rdfs:range orkgc:Evaluation .

# Evaluation → Metric
orkgp:HAS_METRIC a owl:ObjectProperty ;
    rdfs:label "has Metric" ;
    rdfs:domain orkgc:Evaluation ;
    rdfs:range orkgc:Metric .

# Comparison → Contribution
orkgp:compareContribution a owl:ObjectProperty ;
    rdfs:label "compare Contribution" ;
    rdfs:domain orkgc:Comparison ;
    rdfs:range orkgc:Contribution .

# Measurement → Value
orkgp:HAS_VALUE a owl:ObjectProperty ;
    rdfs:label "has Value" ;
    rdfs:domain orkgc:C22007 ;
    rdfs:range orkgc:C22000 .

# Rare / experimental properties
orkgp:P37586 a owl:ObjectProperty ;
    rdfs:label "unknown relation" ;
    rdfs:domain orkgc:Contribution ;
    rdfs:range orkgc:C17021 .
